end)

-- Misc Scripts

local function AutoClaimGift()
	G.Settings.AutoClaimReward = not G.Settings.AutoClaimReward

	if G.Settings.AutoClaimReward == true then
		LDSHUB.AutoClaimFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoClaimFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoClaimReward do
		for i = 1, 12 do
			local args = {
				[1] = tostring(i)
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("TimedRewardService"):WaitForChild("RE"):WaitForChild("onClaim"):FireServer(unpack(args))
			wait(0.5)
		end
		wait(10)
	end
end

local function AutoSping()
	local plr = game.Players.LocalPlayer
	local gui = plr.PlayerGui.GameUI.Menus.Spin.Spin

	if gui.Visible == true then
		G.Settings.AutoSpinReward = true
	else
		G.Settings.AutoSpinReward = false
	end

	if G.Settings.AutoSpinReward == true then
		LDSHUB.AutoClaimFrameColor2.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoClaimFrameColor2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoSpinReward do
		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("SpinService"):WaitForChild("RE"):WaitForChild("onSpinRequest"):FireServer()
		wait(0.1)
		if gui.Visible == false then
			G.Settings.AutoSpinReward = false
		end
	end
end

--Auto Delete

local function FilterButtons(scrollFrame, textBox)
	textBox.Changed:Connect(function()
		local text = textBox.Text:lower()
		if text ~= "" then
			local buttons = scrollFrame:GetDescendants()
			for _, button in pairs(buttons) do
				if button:IsA("TextButton") then
					local buttonText = button.Text:lower()
					if string.find(buttonText, text) then 
						button.Visible = true 
					else 
						button.Visible = false
					end
				end
			end
		else 
			local buttons = scrollFrame:GetDescendants()
			for _, button in pairs(buttons) do
				if button:IsA("TextButton") then
					button.Visible = true
				end
			end
		end
	end)
end

local scrollDelet1 = LDSHUB.ScrollingFramePet1			
local textBoxDelet1 = LDSHUB.TxtBoxPet1
FilterButtons(scrollDelet1, textBoxDelet1)

local scrollDelet2 = LDSHUB.ScrollingFramePet2			
local textBoxDelet2 = LDSHUB.TxtBoxPet2
FilterButtons(scrollDelet2, textBoxDelet2)

local scrollDelet3 = LDSHUB.ScrollingFramePet3			
local textBoxDelet3 = LDSHUB.TxtBoxPet3
FilterButtons(scrollDelet3, textBoxDelet3)


local PetsDelete = game.ReplicatedStorage.Pets.Normal

local function CreateTextButton(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectButtonClickEvent(button, namePetObject)
	button.MouseButton1Click:Connect(function()
		namePetObject.Text = button.Name
	end)
end

for _, petName in pairs(PetsDelete:GetChildren()) do
	local button1 = CreateTextButton(LDSHUB.ScrollingFramePet1, petName.Name)
	ConnectButtonClickEvent(button1, LDSHUB.NamePet)

	local button2 = CreateTextButton(LDSHUB.ScrollingFramePet2, petName.Name)
	ConnectButtonClickEvent(button2, LDSHUB.NamePet_2)

	local button3 = CreateTextButton(LDSHUB.ScrollingFramePet3, petName.Name)
	ConnectButtonClickEvent(button3, LDSHUB.NamePet_3)
end

local function resetsc()
	LDSHUB.PetName1.Visible = false
	LDSHUB.PetName2.Visible = false
	LDSHUB.PetName3.Visible = false
end

LDSHUB.ButtonDelete1.MouseButton1Click:Connect(function()
	resetsc()
	LDSHUB.PetName1.Visible = true
end)

LDSHUB.ButtonDelete2.MouseButton1Click:Connect(function()
	resetsc()
	LDSHUB.PetName2.Visible = true
end)

LDSHUB.ButtonDelete3.MouseButton1Click:Connect(function()
	resetsc()
	LDSHUB.PetName3.Visible = true
end)

local PetInv = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets

local function AutoDelete()
	local namePetR1 = LDSHUB.NamePet.Text
	local namePetR2 = LDSHUB.NamePet_2.Text
	local namePetR3 = LDSHUB.NamePet_3.Text

	if G.Settings.AutoDelete == true then
		LDSHUB.DeleteButtonFrameColor.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.DeleteButtonFrameColor.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoDelete do
		for i, v in pairs(PetInv:GetChildren()) do
			if v:IsA("Frame") then
				wait()
				local toggle = v.Toggle
				if toggle then
					local attributeName = "Key"
					local attributeId = "GUID"

					local attributeValue = toggle:GetAttribute(attributeName)
					local attributeValueid = toggle:GetAttribute(attributeId)

					if attributeValue then
						if attributeValue == namePetR1 or attributeValue == namePetR2 or attributeValue == namePetR3 then
							if attributeValueid then
								local args = {
									[1] = attributeValueid
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PetService"):WaitForChild("RF"):WaitForChild("delete"):InvokeServer(unpack(args))
							end
						end
					end
				end
			end
		end
	end
end

-- Auto Converts

local PetInv = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets

local function AutoCraft()
	for i, v in pairs(PetInv:GetChildren()) do
		if v:IsA("Frame") then
			wait()
			local toggle = v.Toggle
			if toggle then
				local attributeId = "GUID"
				local attributeCraft = "CraftType"

				local attributeValueid = toggle:GetAttribute(attributeId)
				local attributeCraft = toggle:GetAttribute(attributeCraft)

				if attributeValueid then
					if attributeCraft == "Big" or attributeCraft == "Baby" then
						print(attributeValueid)
						wait(2.5)
						local args = {
							[1] = attributeValueid,
							[2] = true
						}

						game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PetService"):WaitForChild("RF"):WaitForChild("craft"):InvokeServer(unpack(args))
					end
					print("huge pet")
				end
			else
				print("pet nao encontrado")
			end
		else
			print("pet nao encontrado")
		end
	end
end

local function AutoCraftPets()

	if G.Settings.AutoCraft == true then
		LDSHUB.ConvertFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.ConvertFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	if G.Settings.AutoCraft == true then
		AutoCraft()
	end
end

LDSHUB.autoconvert2.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = true
	LDSHUB.Wwarn.Visible = true
end)

local function HopGifts()

	if G.Settings.AutoHopSever == true then

		if G.Settings.AutoHopSever == true then
			LDSHUB.AutoClaimFrameColor3.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
		else
			LDSHUB.AutoClaimFrameColor3.AutoClaimFrameColor3 = Color3.fromRGB(255, 0, 0)
		end
		wait(5400)
		for i = 1, 12 do
			local args = {
				[1] = tostring(i)
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("TimedRewardService"):WaitForChild("RE"):WaitForChild("onClaim"):FireServer(unpack(args))
			wait(0.5)
		end
		wait(10)

		local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/13127800756/servers/Public?sortOrder=Asc&limit=100"))
		for i,v in pairs(Servers.data) do
			if v.playing ~= v.maxPlayers then
				game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
			end
		end
	end
end

--Auto Potions


local Potions = {
	"WinPotion",
	"LuckPotion",
	"GoldPotion",
	"VoidPotion"
}


local function CreateTextButtonPotion(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectButtonClickEventPotion(button, namePotionObject)
	button.MouseButton1Click:Connect(function()
		namePotionObject.Text = button.Name
	end)
end

for _, potionName in ipairs(Potions) do
	local button1 = CreateTextButtonPotion(LDSHUB.ScrollingFramePotions1, potionName)
	ConnectButtonClickEventPotion(button1, LDSHUB.PotionNameTxt1)

	local button2 = CreateTextButtonPotion(LDSHUB.ScrollingFramePotions2, potionName)
	ConnectButtonClickEventPotion(button2, LDSHUB.PotionNameTxt2)

	local button3 = CreateTextButtonPotion(LDSHUB.ScrollingFramePotions3, potionName)
	ConnectButtonClickEventPotion(button3, LDSHUB.PotionNameTxt3)

	local button4 = CreateTextButtonPotion(LDSHUB.ScrollingFramePotions4, potionName)
	ConnectButtonClickEventPotion(button4, LDSHUB.PotionNameTxt4)
end

local function resetscPotion()
	LDSHUB.PotionName1.Visible = false
	LDSHUB.PotionName2.Visible = false
	LDSHUB.PotionName3.Visible = false	
	LDSHUB.PotionName4.Visible = false
end

LDSHUB.OpenPotionsButton1.MouseButton1Click:Connect(function()
	resetscPotion()
	LDSHUB.PotionName1.Visible = true
end)

LDSHUB.OpenPotionsButton2.MouseButton1Click:Connect(function()
	resetscPotion()
	LDSHUB.PotionName2.Visible = true
end)

LDSHUB.OpenPotionsButton3.MouseButton1Click:Connect(function()
	resetscPotion()
	LDSHUB.PotionName3.Visible = true
end)

LDSHUB.OpenPotionsButton4.MouseButton1Click:Connect(function()
	resetscPotion()
	LDSHUB.PotionName4.Visible = true
end)

local function AutoPotion1()

	local namePetR1 = LDSHUB.PotionNameTxt1.Text
	local namePetR2 = LDSHUB.PotionNameTxt2.Text
	local namePetR3 = LDSHUB.PotionNameTxt3.Text
	local namePetR4 = LDSHUB.PotionNameTxt4.Text

	if G.Settings.AutoPotions1 == true then
		LDSHUB.PotionFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.PotionFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	if namePetR1 == "WinPotion" or namePetR2 == "WinPotion" or namePetR3 == "WinPotion" or namePetR4  == "WinPotion" then
		while true do
			local args = {
				[1] = "Wins"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BoostService"):WaitForChild("RE"):WaitForChild("useBoost"):FireServer(unpack(args))
			wait(1800)
		end
	end
	if namePetR1 == "LuckPotion" or namePetR2 == "LuckPotion" or namePetR3 == "LuckPotion" or namePetR4  == "LuckPotion" then
		while true do
			local args = {
				[1] = "Luck"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BoostService"):WaitForChild("RE"):WaitForChild("useBoost"):FireServer(unpack(args))
			wait(1800)
		end
	end
	if namePetR1 == "GoldPotion" or namePetR2 == "GoldPotion" or namePetR3 == "GoldPotion" or namePetR4  == "GoldPotion" then
		while true do
			local args = {
				[1] = "Gold"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BoostService"):WaitForChild("RE"):WaitForChild("useBoost"):FireServer(unpack(args))
			wait(1800)
		end
	end
	if namePetR1 == "VoidPotion" or namePetR2 == "VoidPotion" or namePetR3 == "VoidPotion" or namePetR4  == "VoidPotion" then
		while true do
			local args = {
				[1] = "Void"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BoostService"):WaitForChild("RE"):WaitForChild("useBoost"):FireServer(unpack(args))
			wait(1800)
		end
	end
end

local function AutoPotion2()

	if G.Settings.AutoPotions2 == true then
		LDSHUB.PotionFrameColor2.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.PotionFrameColor2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoPotions2 do
		local args = {
			[1] = "Void"
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BoostService"):WaitForChild("RE"):WaitForChild("useBoost"):FireServer(unpack(args))
		local args = {
			[1] = "Luck"
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BoostService"):WaitForChild("RE"):WaitForChild("useBoost"):FireServer(unpack(args))
		local args = {
			[1] = "Gold"
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BoostService"):WaitForChild("RE"):WaitForChild("useBoost"):FireServer(unpack(args))
		local args = {
			[1] = "Wins"
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("BoostService"):WaitForChild("RE"):WaitForChild("useBoost"):FireServer(unpack(args))
		wait(1)
	end
end


local function Rebirths()
	if G.Settings.AutoRebirth == true then
		LDSHUB.MiscFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.MiscFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoRebirth do
		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("RebirthService"):WaitForChild("RE"):WaitForChild("onRebirthRequest"):FireServer()
		wait(1)
	end

end

local function SpinRewards()

	if G.Settings.AutoSpinReward == true then
		LDSHUB.AutoClaimFrameColor2.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoClaimFrameColor2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoSpinReward do
		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("SpinService"):WaitForChild("RE"):WaitForChild("onSpinRequest"):FireServer()
		wait(1)
	end

end

-- Auto Train

local function AutoDumbellKnuckle()

	local BestDumbell = nil 

	local plr = game.Players.LocalPlayer
	local hum = plr.Character.HumanoidRootPart

	local player = game.Players.LocalPlayer

	local character = player.Character
	if not character then
		player.CharacterAdded:Wait()
		character = player.Character
	end

	local humanoid = character:WaitForChild("Humanoid")

	local attributeZone = "BestZone"
	local attributeVip = "VIP"
	local BestZone = plr:GetAttribute(attributeZone)
	local Vip = plr:GetAttribute(attributeVip)

	local sacoVip = game.workspace.Zones[BestZone].Interactables.Training.PunchBags.VIP.Primary.Position

	local function FindBestTier()
		local punchBags = game.Workspace.Zones[BestZone].Interactables.Training.PunchBags
		local bestTier = 0

		for _, punchBag in ipairs(punchBags:GetChildren()) do
			local tier = tonumber(punchBag.Name:match("Tier(%d+)"))

			if tier and tier > bestTier then
				bestTier = tier
			end
		end

		return bestTier
	end

	local bestTier = FindBestTier()

	local sacoNormal = game.workspace.Zones[BestZone].Interactables.Training.PunchBags["Tier" .. bestTier].Primary.Position

	local function Verbestdumbell()

		local dumbells = game.ReplicatedStorage.Tools.Dumbells

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local weights = {}

		-- Obtém todas as ferramentas dentro de game.ReplicatedStorage.Tools.Dumbells
		for _, dumbell in ipairs(dumbells:GetChildren()) do
			local name = dumbell.Name
			local weightStr = name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

			if weightStr then
				-- Remove "Kg" do final do número e converte para um número
				local weight = tonumber(weightStr:sub(1, -3))
				if weight then
					table.insert(weights, weight)
				end
			end
		end

		-- Ordena os números em ordem decrescente
		table.sort(weights, function(a, b) return a > b end)

		local args = {
			[1] = BestZone,
			[2] = "Dumbells",
			[3] = "1Kg"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for i = #weights, 1, -1 do
			args[3] = weights[i] .. "Kg"
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end

	end

	local function SaveBestDumbell()

		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			if character:FindFirstChildWhichIsA("Tool") then
				local equippedTool = character:FindFirstChildWhichIsA("Tool")
				local weightStr = equippedTool.Name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

				if weightStr then
					BestDumbell = weightStr
				end
			end
		end
	end

	local function EquipDumbell()

		while G.Settings.AutoTrain1 do
			local args = {
				[1] = BestZone,
				[2] = "Dumbells",
				[3] = BestDumbell
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest"):FireServer(unpack(args))
			wait()
		end
	end

	local function ATKDumbell()
		while G.Settings.AutoTrain1 do
			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onClick"):FireServer()
			wait()
		end
	end

	local function TPBestZone()

		local locationsFolder = workspace:WaitForChild("Zones")[BestZone]:WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations")

		local firstChild = locationsFolder:GetChildren()[1]

		if firstChild then
			local args = {
				[1] = firstChild
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))
		end
	end

	local function Punch()
		if Vip == true then
			TPBestZone()
			wait(2)
			humanoid:MoveTo(sacoVip)
			wait(1)
			while G.Settings.AutoTrain1 do 
				local args = {
					[1] = BestZone,
					[2] = "VIP",
					[3] = true
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		else
			TPBestZone()
			wait(2)
			humanoid:MoveTo(sacoNormal)
			wait(1)
			while G.Settings.AutoTrain1 do
				local args = {
					[1] = BestZone,
					[2] = bestTier
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		end
	end


	Verbestdumbell()
	wait(1)
	spawn(function()
		SaveBestDumbell()
	end)
	spawn(function()
		EquipDumbell()
	end)
	spawn(function()
		ATKDumbell()
	end)
	spawn(function()
		Punch()
	end)
end

local function AutoBarbellKnuckle()

	local BestBarbell = nil 

	local plr = game.Players.LocalPlayer
	local hum = plr.Character.HumanoidRootPart

	local attributeZone = "BestZone"
	local attributeVip = "VIP"
	local BestZone = plr:GetAttribute(attributeZone)
	local Vip = plr:GetAttribute(attributeVip)

	local sacoVip = game.workspace.Zones[BestZone].Interactables.Training.PunchBags.VIP.Primary.Position

	local function FindBestTier()
		local punchBags = game.Workspace.Zones[BestZone].Interactables.Training.PunchBags
		local bestTier = 0

		for _, punchBag in ipairs(punchBags:GetChildren()) do
			local tier = tonumber(punchBag.Name:match("Tier(%d+)"))

			if tier and tier > bestTier then
				bestTier = tier
			end
		end

		return bestTier
	end

	local bestTier = FindBestTier()

	local sacoNormal = game.workspace.Zones[BestZone].Interactables.Training.PunchBags["Tier" .. bestTier].Primary.Position

	local function VerbestBarbell()
		local barbells = game.ReplicatedStorage.Tools.Barbells

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local tiers = {}

		-- Obtém todas as ferramentas dentro de game.ReplicatedStorage.Tools.Barbells
		for _, barbell in ipairs(barbells:GetChildren()) do
			local name = barbell.Name
			local tier = tonumber(name:match("Tier(%d+)")) -- Extrai o número do nome no padrão "TierX"

			if tier then
				table.insert(tiers, tier)
			end
		end

		-- Ordena os números em ordem crescente
		table.sort(tiers)

		local args = {
			[1] = BestZone,
			[2] = "Barbells",
			[3] = "Tier1"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for _, tier in ipairs(tiers) do
			args[3] = "Tier" .. tier
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end
	end

	local function SaveBestBarbells()
		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			local equippedTool = character:FindFirstChildOfClass("Tool")
			if equippedTool then
				-- Remover "Barbells" do início do nome da ferramenta usando gsub
				local tierName = equippedTool.Name:gsub("Barbells", "")
				BestBarbell = tierName
			end
		end
	end

	local function EquipBarbells()

		while G.Settings.AutoTrain2 do
			local args = {
				[1] = BestZone,
				[2] = "Barbells",
				[3] = BestBarbell
			}
			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onEquipRequest"):FireServer(unpack(args))
			wait()
		end
	end

	local function ATKBarbells()
		while G.Settings.AutoTrain2 do
			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onClick"):FireServer()
			wait()
		end
	end

	local function TPBestZone()

		local locationsFolder = workspace:WaitForChild("Zones")[BestZone]:WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations")

		local firstChild = locationsFolder:GetChildren()[1]

		if firstChild then
			local args = {
				[1] = firstChild
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))
		end
	end

	local function Punch()
		if Vip == true then
			TPBestZone()
			wait(2)
			hum:MoveTo(sacoNormal)
			wait(1)
			while G.Settings.AutoTrain2 do 
				local args = {
					[1] = BestZone,
					[2] = "VIP",
					[3] = true
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		else
			TPBestZone()
			wait(2)
			hum:MoveTo(sacoNormal)
			wait(1)
			while G.Settings.AutoTrain2 do
				local args = {
					[1] = BestZone,
					[2] = bestTier
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		end
	end


	VerbestBarbell()
	wait(1)
	spawn(function()
		SaveBestBarbells()
	end)
	spawn(function()
		EquipBarbells()
	end)
	spawn(function()
		ATKBarbells()
	end)
	spawn(function()
		Punch()
	end)
end

local function AutoHandleKnuckle()

	local BestHandle = nil 

	local plr = game.Players.LocalPlayer
	local hum = plr.Character.HumanoidRootPart

	local player = game.Players.LocalPlayer

	local character = player.Character
	if not character then
		player.CharacterAdded:Wait()
		character = player.Character
	end

	local humanoid = character:WaitForChild("Humanoid")

	local attributeZone = "BestZone"
	local attributeVip = "VIP"
	local BestZone = plr:GetAttribute(attributeZone)
	local Vip = plr:GetAttribute(attributeVip)

	local sacoVip = game.workspace.Zones[BestZone].Interactables.Training.PunchBags.VIP.Primary.Position

	local function FindBestTier()
		local punchBags = game.Workspace.Zones[BestZone].Interactables.Training.PunchBags
		local bestTier = 0

		for _, punchBag in ipairs(punchBags:GetChildren()) do
			local tier = tonumber(punchBag.Name:match("Tier(%d+)"))

			if tier and tier > bestTier then
				bestTier = tier
			end
		end

		return bestTier
	end

	local bestTier = FindBestTier()

	local sacoNormal = game.workspace.Zones[BestZone].Interactables.Training.PunchBags["Tier" .. bestTier].Primary.Position

	local function VerbestHandle()

		local Handles = game.ReplicatedStorage.Tools.Grips

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local weights = {}

		-- Obtém todas as ferramentas dentro de game.ReplicatedStorage.Tools.Dumbells
		for _, Handle in ipairs(Handles:GetChildren()) do
			local name = Handle.Name
			local weightStr = name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

			if weightStr then
				-- Remove "Kg" do final do número e converte para um número
				local weight = tonumber(weightStr:sub(1, -3))
				if weight then
					table.insert(weights, weight)
				end
			end
		end

		-- Ordena os números em ordem decrescente
		table.sort(weights, function(a, b) return a > b end)

		local args = {
			[1] = BestZone,
			[2] = "Grips",
			[3] = "1Kg"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for i = #weights, 1, -1 do
			args[3] = weights[i] .. "Kg"
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end
	end

	local function SaveBestHandle()

		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			if character:FindFirstChildWhichIsA("Tool") then
				local equippedTool = character:FindFirstChildWhichIsA("Tool")
				local weightStr = equippedTool.Name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

				if weightStr then
					BestHandle = weightStr
				end
			end
		end
	end

	local function EquipHandle()

		while G.Settings.AutoTrain3 do
			local args = {
				[1] = BestZone,
				[2] = "Grips",
				[3] = BestHandle
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest"):FireServer(unpack(args))
			wait()
		end
	end

	local function ATKHands()
		while G.Settings.AutoTrain3 do
			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onClick"):FireServer()
			wait()
		end
	end

	local function TPBestZone()

		local locationsFolder = workspace:WaitForChild("Zones")[BestZone]:WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations")

		local firstChild = locationsFolder:GetChildren()[1]

		if firstChild then
			local args = {
				[1] = firstChild
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))
		end
	end

	local function Punch()
		if Vip == true then
			TPBestZone()
			wait(2)
			humanoid:MoveTo(sacoVip)
			wait(1)
			while G.Settings.AutoTrain3 do 
				local args = {
					[1] = BestZone,
					[2] = "VIP",
					[3] = true
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		else
			TPBestZone()
			wait(2)
			humanoid:MoveTo(sacoNormal)
			wait(1)
			while G.Settings.AutoTrain3 do
				local args = {
					[1] = BestZone,
					[2] = bestTier
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		end
	end


	VerbestHandle()
	wait(1)
	spawn(function()
		SaveBestHandle()
	end)
	spawn(function()
		EquipHandle()
	end)
	spawn(function()
		ATKHands()
	end)
	spawn(function()
		Punch()
	end)
end

local function AutoDumbellBarbellKnuckle()

	local BestBarbell = nil 
	local BestDumbell = nil 

	local plr = game.Players.LocalPlayer
	local hum = plr.Character.HumanoidRootPart

	local player = game.Players.LocalPlayer

	local character = player.Character
	if not character then
		player.CharacterAdded:Wait()
		character = player.Character
	end

	local humanoid = character:WaitForChild("Humanoid")

	local attributeZone = "BestZone"
	local attributeVip = "VIP"
	local BestZone = plr:GetAttribute(attributeZone)
	local Vip = plr:GetAttribute(attributeVip)

	local sacoVip = game.workspace.Zones[BestZone].Interactables.Training.PunchBags.VIP.Primary.Position

	local function FindBestTier()
		local punchBags = game.Workspace.Zones[BestZone].Interactables.Training.PunchBags
		local bestTier = 0

		for _, punchBag in ipairs(punchBags:GetChildren()) do
			local tier = tonumber(punchBag.Name:match("Tier(%d+)"))

			if tier and tier > bestTier then
				bestTier = tier
			end
		end

		return bestTier
	end

	local bestTier = FindBestTier()

	local sacoNormal = game.workspace.Zones[BestZone].Interactables.Training.PunchBags["Tier" .. bestTier].Primary.Position

	local function VerbestBarbell()
		local barbells = game.ReplicatedStorage.Tools.Barbells

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local tiers = {}

		-- Obtém todas as ferramentas dentro de game.ReplicatedStorage.Tools.Barbells
		for _, barbell in ipairs(barbells:GetChildren()) do
			local name = barbell.Name
			local tier = tonumber(name:match("Tier(%d+)")) -- Extrai o número do nome no padrão "TierX"

			if tier then
				table.insert(tiers, tier)
			end
		end

		-- Ordena os números em ordem crescente
		table.sort(tiers)

		local args = {
			[1] = BestZone,
			[2] = "Barbells",
			[3] = "Tier1"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for _, tier in ipairs(tiers) do
			args[3] = "Tier" .. tier
			print("Testing " .. args[3]) -- Print para verificar o teste
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end
	end

	local function VerbestDumbell()

		local dumbells = game.ReplicatedStorage.Tools.Dumbells

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local weights = {}

		-- Obtém todas as ferramentas dentro de game.ReplicatedStorage.Tools.Dumbells
		for _, dumbell in ipairs(dumbells:GetChildren()) do
			local name = dumbell.Name
			local weightStr = name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

			if weightStr then
				-- Remove "Kg" do final do número e converte para um número
				local weight = tonumber(weightStr:sub(1, -3))
				if weight then
					table.insert(weights, weight)
				end
			end
		end

		-- Ordena os números em ordem decrescente
		table.sort(weights, function(a, b) return a > b end)

		local args = {
			[1] = BestZone,
			[2] = "Dumbells",
			[3] = "1Kg"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for i = #weights, 1, -1 do
			args[3] = weights[i] .. "Kg"
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end

	end

	local function SaveBestDumbell()

		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			if character:FindFirstChildWhichIsA("Tool") then
				local equippedTool = character:FindFirstChildWhichIsA("Tool")
				local weightStr = equippedTool.Name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

				if weightStr then
					BestDumbell = weightStr
				end
			end
		end
	end

	local function SaveBestBarbells()
		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			local equippedTool = character:FindFirstChildOfClass("Tool")
			if equippedTool then
				-- Remover "Barbells" do início do nome da ferramenta usando gsub
				local tierName = equippedTool.Name:gsub("Barbells", "")
				BestBarbell = tierName
			end
		end
	end

	local function EquipBarbells()
		while G.Settings.AutoTrain4 do
			local args = {
				[1] = BestZone,
				[2] = "Barbells",
				[3] = BestBarbell
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onEquipRequest"):FireServer(unpack(args))
			wait()
		end
	end

	local function EquipDumbell()

		while G.Settings.AutoTrain4 do
			local args = {
				[1] = BestZone,
				[2] = "Dumbells",
				[3] = BestDumbell
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest"):FireServer(unpack(args))
			wait(.2)
		end
	end

	local function ATKBarbells()
		while G.Settings.AutoTrain4 do
			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onClick"):FireServer()
			wait()
		end
	end

	local function TPBestZone()

		local locationsFolder = workspace:WaitForChild("Zones")[BestZone]:WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations")

		local firstChild = locationsFolder:GetChildren()[1]

		if firstChild then
			local args = {
				[1] = firstChild
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))
		end
	end

	local function Punch()
		if Vip == true then
			TPBestZone()
			wait(2)
			humanoid:MoveTo(sacoVip)
			wait(1)
			while G.Settings.AutoTrain4 do 
				local args = {
					[1] = BestZone,
					[2] = "VIP",
					[3] = true
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		else
			TPBestZone()
			wait(2)
			humanoid:MoveTo(sacoNormal)
			wait(1)
			while G.Settings.AutoTrain4 do
				local args = {
					[1] = BestZone,
					[2] = bestTier
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		end
	end


	VerbestBarbell()
	wait(2)
	spawn(function()
		SaveBestBarbells()
	end)
	VerbestDumbell()
	wait(1)
	spawn(function()
		SaveBestDumbell()
	end)
	wait(2)
	spawn(function()
		EquipBarbells()
	end)
	spawn(function()
		EquipDumbell()
	end)
	spawn(function()
		ATKBarbells()
	end)
	spawn(function()
		Punch()
	end)
end

local function AutoDumbellBarbellHandleKnuckle()

	local BestBarbell = nil 
	local BestDumbell = nil 
	local BestHandle = nil 

	local plr = game.Players.LocalPlayer
	local hum = plr.Character.HumanoidRootPart

	local player = game.Players.LocalPlayer

	local character = player.Character
	if not character then
		player.CharacterAdded:Wait()
		character = player.Character
	end

	local humanoid = character:WaitForChild("Humanoid")

	local attributeZone = "BestZone"
	local attributeVip = "VIP"
	local BestZone = plr:GetAttribute(attributeZone)
	local Vip = plr:GetAttribute(attributeVip)

	local sacoVip = game.workspace.Zones[BestZone].Interactables.Training.PunchBags.VIP.Primary.Position

	local function FindBestTier()
		local punchBags = game.Workspace.Zones[BestZone].Interactables.Training.PunchBags
		local bestTier = 0

		for _, punchBag in ipairs(punchBags:GetChildren()) do
			local tier = tonumber(punchBag.Name:match("Tier(%d+)"))

			if tier and tier > bestTier then
				bestTier = tier
			end
		end

		return bestTier
	end

	local bestTier = FindBestTier()

	local sacoNormal = game.workspace.Zones[BestZone].Interactables.Training.PunchBags["Tier" .. bestTier].Primary.Position

	local function VerbestBarbell()
		local barbells = game.ReplicatedStorage.Tools.Barbells

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local tiers = {}

		-- Obtém todas as ferramentas dentro de game.ReplicatedStorage.Tools.Barbells
		for _, barbell in ipairs(barbells:GetChildren()) do
			local name = barbell.Name
			local tier = tonumber(name:match("Tier(%d+)")) -- Extrai o número do nome no padrão "TierX"

			if tier then
				table.insert(tiers, tier)
			end
		end

		-- Ordena os números em ordem crescente
		table.sort(tiers)

		local args = {
			[1] = BestZone,
			[2] = "Barbells",
			[3] = "Tier1"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for _, tier in ipairs(tiers) do
			args[3] = "Tier" .. tier
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end
	end

	local function VerbestDumbell()

		local dumbells = game.ReplicatedStorage.Tools.Dumbells

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local weights = {}

		-- Obtém todas as ferramentas dentro de game.ReplicatedStorage.Tools.Dumbells
		for _, dumbell in ipairs(dumbells:GetChildren()) do
			local name = dumbell.Name
			local weightStr = name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

			if weightStr then
				-- Remove "Kg" do final do número e converte para um número
				local weight = tonumber(weightStr:sub(1, -3))
				if weight then
					table.insert(weights, weight)
				end
			end
		end

		-- Ordena os números em ordem decrescente
		table.sort(weights, function(a, b) return a > b end)

		local args = {
			[1] = BestZone,
			[2] = "Dumbells",
			[3] = "1Kg"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for i = #weights, 1, -1 do
			args[3] = weights[i] .. "Kg"
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end
	end

	local function VerbestHandle()

		local Handles = game.ReplicatedStorage.Tools.Grips

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local weights = {}

		-- Obtém todas as ferramentas dentro de game.ReplicatedStorage.Tools.Dumbells
		for _, Handle in ipairs(Handles:GetChildren()) do
			local name = Handle.Name
			local weightStr = name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

			if weightStr then
				-- Remove "Kg" do final do número e converte para um número
				local weight = tonumber(weightStr:sub(1, -3))
				if weight then
					table.insert(weights, weight)
				end
			end
		end

		-- Ordena os números em ordem decrescente
		table.sort(weights, function(a, b) return a > b end)

		local args = {
			[1] = BestZone,
			[2] = "Grips",
			[3] = "1Kg"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for i = #weights, 1, -1 do
			args[3] = weights[i] .. "Kg"
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end
	end

	local function SaveBestDumbell()

		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			if character:FindFirstChildWhichIsA("Tool") then
				local equippedTool = character:FindFirstChildWhichIsA("Tool")
				local weightStr = equippedTool.Name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

				if weightStr then
					BestDumbell = weightStr
				end
			end
		end
	end

	local function SaveBestBarbells()
		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			local equippedTool = character:FindFirstChildOfClass("Tool")
			if equippedTool then
				-- Remover "Barbells" do início do nome da ferramenta usando gsub
				local tierName = equippedTool.Name:gsub("Barbells", "")
				BestBarbell = tierName
			end
		end
	end

	local function SaveBestHandle()

		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			if character:FindFirstChildWhichIsA("Tool") then
				local equippedTool = character:FindFirstChildWhichIsA("Tool")
				local weightStr = equippedTool.Name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

				if weightStr then
					BestHandle = weightStr
				end
			end
		end
	end

	local function EquipAll()
		while G.Settings.AutoTrain5 do
			local args = {
				[1] = BestZone,
				[2] = "Barbells",
				[3] = BestBarbell
			}
			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onEquipRequest"):FireServer(unpack(args))
			wait(.1)
			local args = {
				[1] = BestZone,
				[2] = "Dumbells",
				[3] = BestDumbell
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest"):FireServer(unpack(args))
			wait(.1)
			local args = {
				[1] = BestZone,
				[2] = "Grips",
				[3] = BestHandle
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest"):FireServer(unpack(args))
			wait(.1)
		end
	end

	local function ATKBarbells()
		while G.Settings.AutoTrain5 do
			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onClick"):FireServer()
			wait()
		end
	end

	local function TPBestZone()

		local locationsFolder = workspace:WaitForChild("Zones")[BestZone]:WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations")

		local firstChild = locationsFolder:GetChildren()[1]

		if firstChild then
			local args = {
				[1] = firstChild
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))
		end
	end

	local function Punch()
		if Vip == true then
			TPBestZone()
			wait(2)
			humanoid:MoveTo(sacoVip)
			wait(1)
			while G.Settings.AutoTrain5 do 
				local args = {
					[1] = BestZone,
					[2] = "VIP",
					[3] = true
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		else
			TPBestZone()
			wait(2)
			humanoid:MoveTo(sacoNormal)
			wait(1)
			while G.Settings.AutoTrain5 do
				local args = {
					[1] = BestZone,
					[2] = bestTier
				}

				game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
				wait()
			end
		end
	end


	VerbestBarbell()
	wait(2)
	spawn(function()
		SaveBestBarbells()
	end)
	wait(1)
	VerbestDumbell()
	wait(2)
	spawn(function()
		SaveBestDumbell()
	end)
	wait(1)
	VerbestHandle()
	wait(2)
	spawn(function()
		SaveBestHandle()
	end)
	spawn(function()
		EquipAll()
	end)
	spawn(function()
		ATKBarbells()
	end)
	spawn(function()
		Punch()
	end)
end

-- Event Atlantis

local function AtlantisDK()

	local BestDumbell = nil 

	local plr = game.Players.LocalPlayer
	local hum = plr.Character.HumanoidRootPart

	local player = game.Players.LocalPlayer

	local character = player.Character
	if not character then
		player.CharacterAdded:Wait()
		character = player.Character
	end

	local humanoid = character:WaitForChild("Humanoid")

	local attributeZone = "BestZone"
	local attributeVip = "VIP"
	local BestZone = plr:GetAttribute(attributeZone)
	local Vip = plr:GetAttribute(attributeVip)

	local function FindBestTier()
		local punchBags = game.Workspace.Zones.AquaEvent.Interactables.Training.PunchBags
		local bestTier = 0

		for _, punchBag in ipairs(punchBags:GetChildren()) do
			local tier = tonumber(punchBag.Name:match("Tier(%d+)"))

			if tier and tier > bestTier then
				bestTier = tier
			end
		end

		return bestTier
	end

	local bestTier = FindBestTier()

	local sacoNormal = game.workspace.Zones.AquaEvent.Interactables.Training.PunchBags["Tier" .. bestTier].Primary.Position

	local function VerbestdumbellEvent()
		local dumbellsEvent = {
			"275Kg",
			"350Kg",
			"450Kg",
			"675Kg",
			"725Kg",
			"950Kg",
			"1350Kg",
			"2200Kg",
			"2600Kg",
			"2750Kg",
			"3400Kg",
			"3750Kg",
		}

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local weights = {}

		for _, weightStr in ipairs(dumbellsEvent) do
			local weight = tonumber(weightStr:sub(1, -3)) -- Remove "Kg" do final do número e converte para um número
			if weight then
				table.insert(weights, weight)
			end
		end

		-- Ordena os números em ordem decrescente
		table.sort(weights, function(a, b) return a > b end)

		local args = {
			[1] = "AquaEvent",
			[2] = "Dumbells",
			[3] = "1Kg"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for i = #weights, 1, -1 do
			args[3] = weights[i] .. "Kg"
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end
	end


	local function SaveBestDumbellEvent()

		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			if character:FindFirstChildWhichIsA("Tool") then
				local equippedTool = character:FindFirstChildWhichIsA("Tool")
				local weightStr = equippedTool.Name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

				if weightStr then
					BestDumbell = weightStr
				end
			end
		end
	end

	local function EquipDumbellEvent()

		while G.Settings.AutoAtlantisEvent2 do
			local args = {
				[1] = "AquaEvent",
				[2] = "Dumbells",
				[3] = BestDumbell
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest"):FireServer(unpack(args))
			wait()
		end
	end

	local function ATKDumbell()
		while G.Settings.AutoAtlantisEvent2 do
			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onClick"):FireServer()
			wait()
		end
	end

	local function TPBestZone()

		local locationsFolder = workspace:WaitForChild("Zones").AquaEvent:WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations")

		local firstChild = locationsFolder:GetChildren()[1]

		if firstChild then
			local args = {
				[1] = firstChild
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))
		end
	end

	local function Punch()
		TPBestZone()
		wait(2)
		humanoid:MoveTo(sacoNormal)
		wait(1)
		while G.Settings.AutoAtlantisEvent2 do
			local args = {
				[1] = "AquaEvent",
				[2] = "Tier5"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
			wait()
		end
	end


	VerbestdumbellEvent()
	wait(1)
	spawn(function()
		SaveBestDumbellEvent()
	end)
	spawn(function()
		EquipDumbellEvent()
	end)
	spawn(function()
		ATKDumbell()
	end)
	spawn(function()
		Punch()
	end)

end

local function AtlantisHK()

	local BestHands = nil 

	local plr = game.Players.LocalPlayer
	local hum = plr.Character.HumanoidRootPart

	local player = game.Players.LocalPlayer

	local character = player.Character
	if not character then
		player.CharacterAdded:Wait()
		character = player.Character
	end

	local humanoid = character:WaitForChild("Humanoid")

	local attributeZone = "BestZone"
	local attributeVip = "VIP"
	local BestZone = plr:GetAttribute(attributeZone)
	local Vip = plr:GetAttribute(attributeVip)

	local function FindBestTier()
		local punchBags = game.Workspace.Zones.AquaEvent.Interactables.Training.PunchBags
		local bestTier = 0

		for _, punchBag in ipairs(punchBags:GetChildren()) do
			local tier = tonumber(punchBag.Name:match("Tier(%d+)"))

			if tier and tier > bestTier then
				bestTier = tier
			end
		end

		return bestTier
	end

	local bestTier = FindBestTier()

	local sacoNormal = game.workspace.Zones.AquaEvent.Interactables.Training.PunchBags["Tier" .. bestTier].Primary.Position

	local function VerbestHandsEvent()
		local HandsEvent = {
			"1Kg",
			"2Kg",
			"3Kg",
			"4Kg",
			"5Kg",
			"10Kg",
			"15Kg",
			"20Kg",
			"25Kg",
			"50Kg",
			"100Kg",
			"250Kg",
		}

		-- Tabela para armazenar os números extraídos dos nomes das ferramentas
		local weights = {}

		for _, weightStr in ipairs(HandsEvent) do
			local weight = tonumber(weightStr:sub(1, -3)) -- Remove "Kg" do final do número e converte para um número
			if weight then
				table.insert(weights, weight)
			end
		end

		-- Ordena os números em ordem decrescente
		table.sort(weights, function(a, b) return a > b end)

		local args = {
			[1] = "AquaEvent",
			[2] = "Grips",
			[3] = "1Kg"
		}

		local remote = game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest")

		for i = #weights, 1, -1 do
			args[3] = weights[i] .. "Kg"
			remote:FireServer(unpack(args))
			wait() -- Aguarda um curto período antes de continuar para evitar sobrecarregar o servidor
		end
	end


	local function SaveBestHandsEvent()

		local player = game.Players.LocalPlayer

		-- Verificar se o personagem do jogador existe
		if player.Character then
			local character = player.Character

			-- Verificar se o personagem tem uma ferramenta equipada
			if character:FindFirstChildWhichIsA("Tool") then
				local equippedTool = character:FindFirstChildWhichIsA("Tool")
				local weightStr = equippedTool.Name:match("%d+Kg") -- Extrai o padrão numérico seguido de "Kg" do nome

				if weightStr then
					BestHands = weightStr
				end
			end
		end
	end

	local function EquipHandsEvent()

		while G.Settings.AutoAtlantisEvent3 do
			local args = {
				[1] = "AquaEvent",
				[2] = "Grips",
				[3] = BestHands
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onGuiEquipRequest"):FireServer(unpack(args))
			wait()
		end
	end

	local function ATKHands()
		while G.Settings.AutoAtlantisEvent3 do
			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ToolService"):WaitForChild("RE"):WaitForChild("onClick"):FireServer()
			wait()
		end
	end

	local function TPBestZone()

		local locationsFolder = workspace:WaitForChild("Zones").AquaEvent:WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations")

		local firstChild = locationsFolder:GetChildren()[1]

		if firstChild then
			local args = {
				[1] = firstChild
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(unpack(args))
		end
	end

	local function Punch()
		TPBestZone()
		wait(2)
		humanoid:MoveTo(sacoNormal)
		wait(1)
		while G.Settings.AutoAtlantisEvent3 do
			local args = {
				[1] = "AquaEvent",
				[2] = "Tier5"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PunchBagService"):WaitForChild("RE"):WaitForChild("onGiveStats"):FireServer(unpack(args))
			wait()
		end
	end


	VerbestHandsEvent()
	wait(1)
	spawn(function()
		SaveBestHandsEvent()
	end)
	spawn(function()
		EquipHandsEvent()
	end)
	spawn(function()
		ATKHands()
	end)
	spawn(function()
		Punch()
	end)

end

-- arms 

local scrollArmsDelet1 = LDSHUB.ScrollingFrameArms1			
local textBoxArmsDelet1 = LDSHUB.TxtBoxArms1
FilterButtons(scrollArmsDelet1, textBoxArmsDelet1)

local scrollArmsDelet2 = LDSHUB.ScrollingFrameArms2			
local textBoxArmsDelet2 = LDSHUB.TxtBoxArms2
FilterButtons(scrollArmsDelet2, textBoxArmsDelet2)

local scrollArmsDelet3 = LDSHUB.ScrollingFrameArms3			
local textBoxArmsDelet3 = LDSHUB.TxtBoxArms3
FilterButtons(scrollArmsDelet3, textBoxArmsDelet3)


local ArmsDelete = game.ReplicatedStorage.Arms

local function CreateTextArmsButton(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectArmsButtonClickEvent(button, namePetObject)
	button.MouseButton1Click:Connect(function()
		namePetObject.Text = button.Name
	end)
end

for _, ArmsName in pairs(ArmsDelete:GetChildren()) do
	local button1 = CreateTextArmsButton(LDSHUB.ScrollingFrameArms1, ArmsName.Name)
	ConnectArmsButtonClickEvent(button1, LDSHUB.NameArm1)

	local button2 = CreateTextArmsButton(LDSHUB.ScrollingFrameArms2, ArmsName.Name)
	ConnectArmsButtonClickEvent(button2, LDSHUB.NameArm2)

	local button3 = CreateTextArmsButton(LDSHUB.ScrollingFrameArms3, ArmsName.Name)
	ConnectArmsButtonClickEvent(button3, LDSHUB.NameArm3)
end

local function resetArmssc()
	LDSHUB.ArmsName1.Visible = false
	LDSHUB.ArmsName2.Visible = false
	LDSHUB.ArmsName3.Visible = false
end

LDSHUB.ArmsButtonDelete1.MouseButton1Click:Connect(function()
	resetArmssc()
	LDSHUB.ArmsName1.Visible = true
end)

LDSHUB.ArmsButtonDelete2.MouseButton1Click:Connect(function()
	resetArmssc()
	LDSHUB.ArmsName2.Visible = true
end)

LDSHUB.ArmsButtonDelete3.MouseButton1Click:Connect(function()
	resetArmssc()
	LDSHUB.ArmsName3.Visible = true
end)

local PetInv = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.Arms.Container.Arms

local function AutoArmsDelete()
	local namePetR1 = LDSHUB.NameArm1.Text
	local namePetR2 = LDSHUB.NameArm2.Text
	local namePetR3 = LDSHUB.NameArm3.Text
	
	while G.Settings.AutoDeleteArms do
		for i, v in pairs(PetInv:GetChildren()) do
			if v:IsA("Frame") then
				wait()
				local toggle = v.Toggle
				if toggle then
					local attributeName = "ArmDisplayName"

					local attributeValue = toggle:GetAttribute(attributeName)
					local attributeValueid = v.Name

					if attributeValue then
						if attributeValue == namePetR1 or attributeValue == namePetR2 or attributeValue == namePetR3 then
							if attributeValueid then
								local args = {
									[1] = {
										[1] = attributeValueid 
									}
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmsService"):WaitForChild("RF"):WaitForChild("DeleteArms"):InvokeServer(unpack(args))

							end
						end
					end
				end
			end
		end
	end
end

local crates = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.Arms.Container.Crates

local scrollcrateselet = LDSHUB.ScrollingFrameCrate1			
local textBoxcrate = LDSHUB.TxtBoxCrate1
FilterButtons(scrollcrateselet, textBoxcrate)


local function CreateTextCratesButton(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectCratesButtonClickEvent(button, namePetObject)
	button.MouseButton1Click:Connect(function()
		namePetObject.Text = button.Name
	end)
end

for _, cratesName in pairs(crates:GetChildren()) do
	if cratesName:IsA("Frame") then
		local crateName = cratesName.Name
		-- Verifica se o nome da caixa contém "Key" e ignora a criação do botão se for o caso
		if not string.find(crateName, "Key") then
			local button = CreateTextCratesButton(LDSHUB.ScrollingFrameCrate1, crateName)
			ConnectCratesButtonClickEvent(button, LDSHUB.NameCrate)
		end
	end
end


local crates = {
	c1x = false,
	c10x = false,
	c100x = false,
}

local function resetCrates()
	for key, _ in pairs(crates) do
		crates[key] = false
	end
end

local function vercores()
	if crates.c1x == true then
		LDSHUB.Crate1xFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.Crate1xFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	if crates.c10x == true then
		LDSHUB.Crate10xFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.Crate10xFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	if crates.c100x == true then
		LDSHUB.Crate100xFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.Crate100xFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
end

LDSHUB.Crate1x.MouseButton1Click:Connect(function()
	resetCrates()
	if not crates.c10x and not crates.c100x then
		crates.c1x = not crates.c1x
		vercores()
	end
end)

LDSHUB.Crate10x.MouseButton1Click:Connect(function()
	resetCrates()
	if not crates.c1x and not crates.c100x then
		crates.c10x = not crates.c10x
		vercores()
	end
end)

LDSHUB.Crate100x.MouseButton1Click:Connect(function()
	resetCrates()
	if not crates.c1x and not crates.c10x then
		crates.c100x = not crates.c100x
		vercores()
	end
end)

local function AutoBuyCrate()
	
	local allCratesAreFalse = true

	for _, value in pairs(crates) do
		if value == true then
			allCratesAreFalse = false
			break
		end
	end
	
	if allCratesAreFalse then
		LDSHUB.Warn.Visible = true
		LDSHUB.Swarn.Visible = true
	else
	while G.Settings.AutoBuyCrates do
		wait(1) -- Espere por 1 segundo antes de comprar novamente
		if crates.c1x == true then
			local args = {
				[1] = LDSHUB.NameCrate.Text,
				[2] = "1"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmsService"):WaitForChild("RF"):WaitForChild("PurchaseCrates"):InvokeServer(unpack(args))

		end
		if crates.c10x == true then
			local args = {
				[1] = LDSHUB.NameCrate.Text,
				[2] = "10"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmsService"):WaitForChild("RF"):WaitForChild("PurchaseCrates"):InvokeServer(unpack(args))

		end
		if crates.c100x == true then
			local args = {
				[1] = LDSHUB.NameCrate.Text,
				[2] = "100"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmsService"):WaitForChild("RF"):WaitForChild("PurchaseCrates"):InvokeServer(unpack(args))

			end
		end
	end
end

