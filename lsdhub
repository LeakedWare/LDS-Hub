LDSHUB.AutoTrain1.MouseButton1Click:Connect(function()
	LDSHUB.AutoTrainFrame1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	G.Settings.AutoTrain1 = not G.Settings.AutoTrain1
	if G.Settings.AutoTrain1 == true then
		AutoDumbellKnuckle()
	else
		LDSHUB.AutoTrainFrame1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
end)

LDSHUB.AutoTrain2.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = true
	LDSHUB.Pwarn.Visible = true
end)

LDSHUB.AutoTrain3.MouseButton1Click:Connect(function()
	LDSHUB.AutoTrainFrame3.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	G.Settings.AutoTrain3 = not G.Settings.AutoTrain3
	if G.Settings.AutoTrain3 == true then
		AutoHandleKnuckle()
	else
		LDSHUB.AutoTrainFrame3.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
end)

LDSHUB.AutoTrain4.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = true
	LDSHUB.Pwarn.Visible = true
end)

LDSHUB.AutoTrain5.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = true
	LDSHUB.Pwarn.Visible = true
end)

LDSHUB.ResetSaveButton.MouseButton1Click:Connect(function()
	spawn(function()
		ResetHub()
	end)
	wait(1)
	spawn(function()
		SaveSettings()
	end)
end)

-- Verificar o Save
wait(2)
local function executeFunction(func)
	if type(func) == "function" then
		func()
	end
end

local functionsToExecute = {
	["AutoEvent1"] = AutoEvent1,
	["AutoEvent2"] = AutoEvent2,
	["AutoEvent3"] = AutoEvent3,
	["AutoEvent4"] = AutoEvent4,
	["AutoClick"] = AutoClick,
	["AutoIndex"] = AutoIndex, 
	["AutoBoss"] = AutoBoss,
	["AutoEgg"] = AutoEgg,
	["RemoveEggAnimation"] = VisibleEgg,
	["AutoClaimReward"] = AutoClaimGift,
	["AutoSpinReward"] = AutoSping,
	["AutoDelete"] = AutoDelete,
	["AutoCraft"] = AutoCraftPets,
	["AutoTrain1"] = AutoDumbellKnuckle,
	["AutoTrain2"] = AutoBarbellKnuckle,
	["AutoTrain3"] = AutoHandleKnuckle,
	["AutoTrain4"] = AutoDumbellBarbellKnuckle,
	["AutoTrain5"] = AutoDumbellBarbellHandleKnuckle,
	["AutoHopSever"] = HopGifts,
	["AutoDeleteArms"] = AutoArmsDelete,
	["AutoBuyCrates"] = AutoBuyCrate
}

for key, func in pairs(functionsToExecute) do
	if G.Settings[key] then
		executeFunction(func)
	end
end
