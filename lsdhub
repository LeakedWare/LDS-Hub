local textBoxEgg = LDSHUB.TxtBoxAutoFarm

textBoxEgg.Changed:Connect(function()
	local text = textBoxEgg.Text:lower()
	if text ~= "" then
		local buttons = scrollEgg:GetDescendants()
		for _, button in pairs(buttons) do
			if button:IsA("TextButton") then
				local buttonText = button.Text:lower()
				if string.find(buttonText, text) then 
					button.Visible = true 
				else 
					button.Visible = false
				end
			end
		end
	else 
		local buttons = scrollEgg:GetDescendants()
		for _, button in pairs(buttons) do
			if button:IsA("TextButton") then
				button.Visible = true
			end
		end
	end
end)

local function CreateTextButtonEgg(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectButtonClickegg(button, namePetObject)
	button.MouseButton1Click:Connect(function()
		namePetObject.Text = button.Name
	end)
end

local zones = game.Workspace.Zones

-- Obtém todas as pastas numeradas dentro de game.Workspace.Zones
local numberedFolders = {}
for _, folder in ipairs(zones:GetChildren()) do
	if folder:IsA("Folder") then
		if folder.Name then
			table.insert(numberedFolders, folder)
		end
	end
end

-- Itera sobre as pastas numeradas
for _, folder in ipairs(numberedFolders) do
	local mapFolder = folder:FindFirstChild("Map")
	if mapFolder then
		-- Obtenha a pasta "Eggs" dentro de mapFolder
		local eggsInMap = mapFolder:FindFirstChild("Eggs")
		if eggsInMap then
			-- Faça algo com a pasta "Eggs" dentro de "Map" (por exemplo, listar os ovos)
			for _, egg in ipairs(eggsInMap:GetChildren()) do
				local eggName = egg.Name:gsub(" Egg$", "") -- Remove " Egg" do final do nome
				eggName = eggName:gsub("Egg$", "") -- Remove "Egg" do final do nome (sem espaço)
				local button = CreateTextButtonEgg(LDSHUB.ScrollingFrameEgg, eggName)
				ConnectButtonClickegg(button, LDSHUB.NameEgg) -- Modificado aqui para passar diretamente LDSHUB.NameEgg
			end
		end
	end

	local interactablesFolder = folder:FindFirstChild("Interactables")
	if interactablesFolder then
		-- Obtenha a pasta "Eggs" dentro de interactablesFolder
		local eggsInInteractables = interactablesFolder:FindFirstChild("Eggs")
		if eggsInInteractables then
			-- Faça algo com a pasta "Eggs" dentro de "Interactables" (por exemplo, listar os ovos)
			for _, egg in ipairs(eggsInInteractables:GetChildren()) do
				local eggName = egg.Name:gsub(" Egg$", "") -- Remove " Egg" do final do nome
				eggName = eggName:gsub("Egg$", "") -- Remove "Egg" do final do nome (sem espaço)
				local button = CreateTextButtonEgg(LDSHUB.ScrollingFrameEgg, eggName)
				ConnectButtonClickegg(button, LDSHUB.NameEgg) -- Modificado aqui para passar diretamente LDSHUB.NameEgg
			end
		end
	end
end

local function AutoEgg()

	if G.Settings.AutoEgg == true then
		LDSHUB.AutoEggFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoEggFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoEgg do
		local args = {
			[1] = LDSHUB.NameEgg.Text,
			[2] = {},
			[4] = false
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
		wait(1.5)
	end
end

local function VisibleEgg()
	local plr = game.Players.LocalPlayer
	local gui = plr.PlayerGui.OpenerUI


	if G.Settings.RemoveEggAnimation == true then
		LDSHUB.VisibleEggFrameColor.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.VisibleEggFrameColor.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	if G.Settings.RemoveEggAnimation == true then
		gui.Enabled = false
	else
		gui.Enabled = true
	end
end

