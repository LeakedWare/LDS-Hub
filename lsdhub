	LDSHUB.Warn.Visible = true
	LDSHUB.Wwarn.Visible = true
end)

LDSHUB.AutoDeleteArmsButton.MouseButton1Click:Connect(function()
	G.Settings.AutoDeleteArms = not G.Settings.AutoDeleteArms
	if G.Settings.AutoDeleteArms == true then
		LDSHUB.DeleteFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	while G.Settings.AutoDeleteArms do
		AutoArmsDelete()
		wait(2)
		end
	else
		LDSHUB.DeleteFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
end)

LDSHUB.AutobuyCratesButton1.MouseButton1Click:Connect(function()
	G.Settings.AutoBuyCrates = not G.Settings.AutoBuyCrates
	if G.Settings.AutoBuyCrates then
		LDSHUB.CrateFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	while G.Settings.AutoBuyCrates do
		AutoBuyCrate()
		wait(2)
		end
	else
		LDSHUB.CrateFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
end)

