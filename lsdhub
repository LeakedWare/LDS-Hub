	"Phoenix",
	"Bolt",
	"Thunder"
}

local World1 = true
local World2 = true
local World3 = false

local PetIndex = game.Players.LocalPlayer.PlayerGui.GameUI.Menus.PetIndex.Container.ScrollingFrame.Pets

local function verificarPet(child)
	local toggle = child.Toggle
	if toggle then
		local attributeName = "DisplayName"
		local attributeValue = toggle:GetAttribute(attributeName)
		if attributeValue then
			print("Novo Pet Adicionado: " .. attributeValue)
		end
	end
end

local function checkPetIndex(index)
	local v = PetIndex:GetChildren()[index]
	if v and v:IsA("Frame") then
		local toggle = v.Toggle
		local PetIcon = toggle.PetIcon
		if toggle then
			local attributeKey = "Key"
			local attributeValueKey = toggle:GetAttribute(attributeKey)
			print("verificando pet")
			if attributeValueKey then
				if PetIcon.ImageColor3 == Color3.new(0, 0, 0) then
					-- Verificar se o nome do animal de estimação está presente em cada tabela de ovos

					-- World 1 Eggs
					if World1 == true then
						local isInEarthEgg = table.find(EarthEgg, attributeValueKey)
						local isInIcyEgg = table.find(IcyEgg, attributeValueKey)
						local isInLavaEgg = table.find(LavaEgg, attributeValueKey)
						local isInBlackholeEgg = table.find(BlackholeEgg, attributeValueKey)

						if isInEarthEgg then
							repeat
								local args = {
									[1] = "Earth",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInIcyEgg then
							repeat
								local args = {
									[1] = "Icy",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInLavaEgg then
							repeat
								local args = {
									[1] = "Lava",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInBlackholeEgg then
							repeat
								local args = {
									[1] = "Blackhole",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
					end

					if World2 == true then
						local isInCrystalEgg = table.find(CrystalEgg, attributeValueKey)
						local isInMoltenEgg = table.find(MoltenEgg, attributeValueKey)
						local isInSolarEgg = table.find(SolarEgg, attributeValueKey)
						local isInMoonEgg = table.find(MoonEgg, attributeValueKey)
						local isInIceEgg = table.find(IceEgg, attributeValueKey)
						local isInBurningEgg = table.find(BurningEgg, attributeValueKey)

						if isInCrystalEgg then
							repeat
								local args = {
									[1] = "Crystal",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInMoltenEgg then
							repeat
								local args = {
									[1] = "Molten",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInSolarEgg then
							repeat
								local args = {
									[1] = "Solar",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInMoonEgg then
							repeat
								local args = {
									[1] = "Moon",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInIceEgg then
							repeat
								local args = {
									[1] = "Ice",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInBurningEgg then
							repeat
								local args = {
									[1] = "Burning",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
					end

					if World3 == true then
						local isInCoconutEgg = table.find(CoconutEgg, attributeValueKey)
						local isInPalmEgg = table.find(PalmEgg, attributeValueKey)
						local isInTreasureEgg = table.find(TreasureEgg, attributeValueKey)
						local isInClamEgg = table.find(ClamEgg, attributeValueKey)
						local isInKingFishEgg = table.find(KingFishEgg, attributeValueKey)
						local isInPoseidonEgg = table.find(PoseidonEgg, attributeValueKey)

						if isInCoconutEgg then
							repeat
								local args = {
									[1] = "Coconut",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInPalmEgg then
							repeat
								local args = {
									[1] = "Palm",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInTreasureEgg then
							repeat
								local args = {
									[1] = "Treasure",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInClamEgg then
							repeat
								local args = {
									[1] = "Clam",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet()
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInKingFishEgg then
							repeat
								local args = {
									[1] = "KingFish",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end
						if isInPoseidonEgg then
							repeat
								local args = {
									[1] = "Poseidon",
									[2] = {},
									[4] = false
								}

								game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
								wait(1)
								verificarPet(v)
							until PetIcon.ImageColor3 == Color3.new(1, 1, 1) or G.Settings.AutoIndex == false
						end 
					end
				end
			end
		end
	end
	wait(1)
	checkPetIndex(index + 1)
end

local function AutoIndex()

	if G.Settings.AutoIndex == true then
		LDSHUB.AutoFarmFrameColor2.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoFarmFrameColor2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	if G.Settings.AutoIndex == true then	
		checkPetIndex(1)
	end
end

-- Auto Boss 

local scrollBoss = LDSHUB.ScrollingFrameBoss
local textBoxBoss = LDSHUB.TxtBoxBoss

textBoxBoss.Changed:Connect(function()
	local text = textBoxBoss.Text:lower()
	if text ~= "" then
		local buttons = scrollBoss:GetDescendants()
		for _, button in pairs(buttons) do
			if button:IsA("TextButton") then
				local buttonText = button.Text:lower()
				if string.find(buttonText, text) then 
					button.Visible = true 
				else 
					button.Visible = false
				end
			end
		end
	else 
		local buttons = scrollBoss:GetDescendants()
		for _, button in pairs(buttons) do
			if button:IsA("TextButton") then
				button.Visible = true
			end
		end
	end
end)

local selectedBossName = ""

local function CreateTextButtonBoss(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectButtonClickEventBoss(button, namePetObject)
	button.MouseButton1Click:Connect(function()
		namePetObject.Text = button.Name
		selectedBossName = button.Name -- Atualiza o valor da variável selectedBossName
	end)
end

local Worlds = game.Workspace.Zones

for _, world in ipairs(Worlds:GetChildren()) do
	if world:IsA("Folder") then
		local ArmWrestling = world.Interactables.ArmWrestling
		if ArmWrestling and ArmWrestling:FindFirstChild("NPC") then
			local Bosses = ArmWrestling.NPC
			for _, NameBoss in ipairs(Bosses:GetChildren()) do
				local button = CreateTextButtonBoss(LDSHUB.ScrollingFrameBoss, NameBoss.Name)
				ConnectButtonClickEventBoss(button, LDSHUB.NameBoss)
			end
		end
	end
end

local function FindZoneNumber(bossName)
	local Worlds = game.Workspace.Zones

	for _, world in ipairs(Worlds:GetChildren()) do
		if world:IsA("Folder") then
			local ArmWrestling = world.Interactables.ArmWrestling
			if ArmWrestling and ArmWrestling:FindFirstChild("NPC") then
				local Bosses = ArmWrestling.NPC
				for _, NameBoss in ipairs(Bosses:GetChildren()) do
					if NameBoss.Name == bossName then
						return world.Name
					end
				end
			end
		end
	end

	-- Se o chefe não foi encontrado em nenhuma zona, retorne nil ou um valor padrão de sua escolha
	return nil
end

local function AutoBoss()
	if G.Settings.AutoBoss == true then
		LDSHUB.AutoFarmFrameColor3.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoFarmFrameColor3.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoBoss do
		wait()
		local bossName = selectedBossName -- Usa o nome do chefe selecionado na variável selectedBossName
		local zoneNumber = FindZoneNumber(bossName)
		if zoneNumber then
			local args = {
				[1] = bossName,
				[2] = game.Workspace.Zones[tostring(zoneNumber)]:WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(bossName),
				[3] = zoneNumber
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
		end
	end
end


