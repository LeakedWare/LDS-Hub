LDSHUB.HomeUi.Visible = true

--Event script
local EggEventsName = {
	"CrayonEgg",
	"HologramEgg",
	"FrostEgg"
}

local function CreateTextButtonEggsEvent(parent, name)
	local TextButton = Instance.new("TextButton")
	TextButton.Name = name
	TextButton.Parent = parent
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.Size = UDim2.new(0.9, 0, 0.001, 0)
	TextButton.Font = Enum.Font.SciFi
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14
	TextButton.TextWrapped = true

	local UICorner = Instance.new("UICorner")
	UICorner.Parent = TextButton

	return TextButton
end

local function ConnectButtonClickEventEggsEvent(button, nameEggObject)
	button.MouseButton1Click:Connect(function()
		nameEggObject.Text = button.Name
	end)
end

for _, EggsEventName in ipairs(EggEventsName) do
	local button1 = CreateTextButtonEggsEvent(LDSHUB.ScrollingFramePetsEvent, EggsEventName)
	ConnectButtonClickEventEggsEvent(button1, LDSHUB.EggEventTxt)
end

LDSHUB.EggEventButton.MouseButton1Click:Connect(function()
	LDSHUB.EggEventName.Visible = not LDSHUB.EggEventName.Visible
end)

local function AutoEvent1()
	if G.Settings.AutoEvent1 == true then
		LDSHUB.EventFrame1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.EventFrame1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	while G.Settings.AutoEvent1 do
		wait()

		if LDSHUB.EggEventTxt.Text == "CrayonEgg" then

			local args = {
				[1] = 8,
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EventService"):WaitForChild("RF"):WaitForChild("ClaimEgg"):InvokeServer(unpack(args))

		elseif LDSHUB.EggEventTxt.Text == "HologramEgg" then

			local args = {
				[1] = 8,
				[2] = true
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EventService"):WaitForChild("RF"):WaitForChild("ClaimEgg"):InvokeServer(unpack(args))
		elseif LDSHUB.EggEventTxt.Text == "FrostEgg" then

			local args = {
				[1] = 8
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EventService"):WaitForChild("RF"):WaitForChild("ClaimEgg"):InvokeServer(unpack(args))

		end
	end
end

local function AutoEvent2()

	if G.Settings.AutoEvent2 == true then
		LDSHUB.EventFrame2.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.EventFrame2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	while G.Settings.AutoEvent2 do
		local Ui = game.Players.LocalPlayer.PlayerGui.GameUI.Wrestle
		local Numeros = 1
		local npcData = {
			[1] = {
				name = 	   "RipperDoc",
				npc = nil,
			}
		}
		repeat
			if Ui.Visible == false then
				local data = npcData[Numeros]
				local npc = workspace:WaitForChild("Zones"):WaitForChild("2"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(data.name)
				if npc then
					data.npc = npc
					local args = {
						[1] = data.name,
						[2] = npc,
						[3] = "2"
					}	
					game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
					-- Aguardar até que o NPC seja derrotado
					wait(2)
					repeat
						wait()
					until Ui.Visible == false
				end
			end
			-- Passar para o próximo NPC
			Numeros = Numeros + 1
		until Numeros > #npcData
		wait(0.1)
	end
end

local function AutoEvent3()

	if G.Settings.AutoEvent3 == true then
		LDSHUB.EventFrame3.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.EventFrame3.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	while G.Settings.AutoEvent3 do
		local Ui = game.Players.LocalPlayer.PlayerGui.GameUI.Wrestle
		local Numeros = 1
		local npcData = {
			[1] = {
				name = 	  "CocoNut",
				npc = nil,
			}
		}
		repeat
			if Ui.Visible == false then
				local data = npcData[Numeros]
				local npc = workspace:WaitForChild("Zones"):WaitForChild("3"):WaitForChild("Interactables"):WaitForChild("ArmWrestling"):WaitForChild("NPC"):WaitForChild(data.name)
				if npc then
					data.npc = npc
					local args = {
						[1] = data.name,
						[2] = npc,
						[3] = "3"
					}	
					game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onEnterNPCTable"):FireServer(unpack(args))
					-- Aguardar até que o NPC seja derrotado
					wait(2)
					repeat
						wait()
					until Ui.Visible == false
				end
			end
			-- Passar para o próximo NPC
			Numeros = Numeros + 1
		until Numeros > #npcData
		wait(0.1)
	end
end

local function AutoEvent4()
	if G.Settings.AutoEvent4 == true then
		LDSHUB.EventFrame5.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.EventFrame5.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end
	while G.Settings.AutoEvent4 do
		wait()
		wait()
		local args = {
			[1] = "Limited",
			[4] = false
		}

		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))

	end
end

-- AutoFarm scripts

local function AutoClick()

	if G.Settings.AutoClick == true then
		LDSHUB.AutoFarmFrameColor1.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	else
		LDSHUB.AutoFarmFrameColor1.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	while G.Settings.AutoClick == true do
		wait(0.1)
		game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ArmWrestleService"):WaitForChild("RE"):WaitForChild("onClickRequest"):FireServer()
	end
end

