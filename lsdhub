--scripts >

--Saves

local G = getgenv()
G.Settings = {
	AutoEvent1 = false,
	AutoEvent2 = false,
	AutoEvent3 = false,
	AutoEvent4 = false,
	AutoAtlantisEvent2 = false,
	AutoAtlantisEvent3 = false,
	AutoSeasonPass1 = false,
	AutoSeasonPass2 = false,
	AutoSeasonPass3 = false,
	AutoClick = false,
	AutoIndex = false,
	AutoBoss = false,
	AutoEgg = false,
	RemoveEggAnimation = false,
	AutoClaimReward = false,
	AutoSpinReward = false,
	AutoHopSever = false,
	AutoRebirth = false,
	AutoDelete = false,
	AutoPotions1 = false,
	AutoPotions2 = false,
	AutoTrain1 = false,
	AutoTrain2 = false,
	AutoTrain3 = false,
	AutoTrain4 = false,
	AutoTrain5 = false,
	AutoCraft = false,
	boostFPS = false,
	AutoDeleteArms = false,
	AutoBuyCrates = false
};

local function ResetHub()
	G.Settings.AutoEvent1 = false
	G.Settings.AutoEvent2 = false
	G.Settings.AutoEvent3 = false
	G.Settings.AutoEvent4 = false
	G.Settings.AutoAtlantisEvent2 = false
	G.Settings.AutoAtlantisEvent3 = false
	G.Settings.AutoSeasonPass1 = false
	G.Settings.AutoSeasonPass2 = false
	G.Settings.AutoSeasonPass3 = false
	G.Settings.AutoClick = false
	G.Settings.AutoIndex = false
	G.Settings.AutoBoss = false
	G.Settings.AutoEgg = false
	G.Settings.RemoveEggAnimation = false
	G.Settings.AutoClaimReward = false
	G.Settings.AutoSpinReward = false
	G.Settings.AutoHopSever = false
	G.Settings.AutoRebirth = false
	G.Settings.AutoPotions1 = false
	G.Settings.AutoPotions2 = false
	G.Settings.AutoDelete = false
	G.Settings.AutoTrain1 = false
	G.Settings.AutoTrain2 = false
	G.Settings.AutoTrain3 = false
	G.Settings.AutoTrain4 = false
	G.Settings.AutoTrain5 = false
	G.Settings.AutoCraft = false
	G.Settings.boostFPS = false
	G.Settings.AutoDeleteArms = false
	G.Settings.AutoBuyCrates = false
end

local NameSave = game.Players.LocalPlayer.Name .. "AWSomega1.0"

local function LoadSettings()
	local HttpService = game:service('HttpService')

	--use 'isfile' to check if we are reading the correct txt file

	if isfile(NameSave) then
		--use 'readfile' to read 'TUTORIAL.txt' and decode it
		G.Settings = HttpService:JSONDecode(readfile(NameSave))
	end
end

local function SaveSettings()
	local JSON -- is nil
	local HttpService = game:service('HttpService') -- used for the JSON DECODE AND ENCODE
	--Decode
	JSON = HttpService:JSONEncode(G.Settings)
	--Make a text file using 'writefile'
	writefile(NameSave,JSON)
end

LoadSettings()

wait(1)

--Geral
LDSHUB.SaveButton.MouseButton1Click:Connect(function()
	SaveSettings()
	LDSHUB.SaveFrameColor.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	wait(2)
	LDSHUB.SaveFrameColor.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
end)

LDSHUB.DeleteButton.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = true
	LDSHUB.Dwarn.Visible = true
	ResetHub()
end)

LDSHUB.StatusButton.MouseButton1Click:Connect(function()
	LDSHUB.StatusFrameColor.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
	loadstring(game:HttpGet("https://raw.githubusercontent.com/LDSHUBscripts/scripts/main/Status"))()
	wait(2)
	LDSHUB.StatusFrameColor.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
end)

LDSHUB.Yes.MouseButton1Click:Connect(function()
	LDSHUB.LDSHUB:Destroy()
end)

LDSHUB.No.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = false
	LDSHUB.Dwarn.Visible = false
end)

LDSHUB.Okay.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = false
	LDSHUB.Wwarn.Visible = false
end)

LDSHUB.Okay_2.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = false
	LDSHUB.Swarn.Visible = false
end)

LDSHUB.OkayP.MouseButton1Click:Connect(function()
	LDSHUB.Warn.Visible = false
	LDSHUB.Pwarn.Visible = false
end)

LDSHUB.TextButton.MouseButton1Click:Connect(function()
	LDSHUB.Frame.Visible = not LDSHUB.Frame.Visible
	if LDSHUB.Frame.Visible == false then
		LDSHUB.TextButton.Text = "+"
	else
		LDSHUB.TextButton.Text = "-"
	end
end)

LDSHUB.AntAFKButton.MouseButton1Click:Connect(function()
	LDSHUB.AntiAFKFrameColor.BackgroundColor3 = Color3.fromRGB(85, 255, 0)

	local bb=game:service'VirtualUser'
	game:service'Players'.LocalPlayer.Idled:connect(function()
		bb:CaptureController()bb:ClickButton2(Vector2.new())
	end)

end)

LDSHUB.FPSButton.MouseButton1Click:Connect(function()
	LDSHUB.FPSFrameColor.BackgroundColor3 = Color3.fromRGB(85, 255, 0)

	local vim = game:GetService("VirtualInputManager")
	setfpscap(5000)

	game.DescendantAdded:Connect(function(d)
		if d.Name == "MainView" and d.Parent.Name == "DevConsoleUI" and G.Settings.boostFPS then
			task.wait()
			local screen = d.Parent.Parent.Parent
			screen.Enabled = false;
			d.Visible = false;
		end
	end)

	vim:SendKeyEvent(true, "F9", 0, game)    
	wait()
	vim:SendKeyEvent(false, "F9", 0, game)  

	while true do
		task.wait()
		if not G.Settings.boostFPS then
			continue;
		end

		warn("")
	end

	local decalsyeeted = true
	local g = game
	local w = g.Workspace
	local l = g.Lighting
	local t = w.Terrain
	t.WaterWaveSize = 0
	t.WaterWaveSpeed = 0
	t.WaterReflectance = 0
	t.WaterTransparency = 0
	l.GlobalShadows = false
	l.FogEnd = 9e9
	l.Brightness = 0
	settings().Rendering.QualityLevel = "Level01"
	for i, v in pairs(g:GetDescendants()) do
		if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
		elseif v:IsA("Decal") or v:IsA("Texture") and decalsyeeted then
			v.Transparency = 1
		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
			v.Lifetime = NumberRange.new(0)
		elseif v:IsA("Explosion") then
			v.BlastPressure = 1
			v.BlastRadius = 1
		elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") or v:IsA("Sparkles") then
			v.Enabled = false
		elseif v:IsA("MeshPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
			v.TextureID = 10385902758728957
		end
	end
	for i, e in pairs(l:GetChildren()) do
		if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then
			e.Enabled = false
		end
	end
end)

LDSHUB.Frame.Draggable = true
LDSHUB.Frame.Selectable = true
LDSHUB.Frame.Active = true

--ToogleGui

local FrameObject = LDSHUB.Frame
local Open = false

local PositionClosed = UDim2.new(-0.317, 0,0351, 0)
local PositionOpen = UDim2.new(0, 407,0, 255)

local UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:Connect(function(KeyCode)
	if KeyCode.KeyCode == Enum.KeyCode.RightShift then
		if Open then
			FrameObject:TweenPosition((PositionClosed), "Out", "Quart")
			Open = false
		else
			Open = true
			FrameObject:TweenPosition((PositionOpen), "Out", "Quint")
		end
	end
end)

--Abrir UI
local function toggleFrame(frame, button)
	local isVisible = not frame.Visible
	frame.Visible = isVisible
	button.BackgroundColor3 = isVisible and Color3.fromRGB(170, 0, 255) or Color3.fromRGB(126, 126, 126)
end

local function hideFrames()
	LDSHUB.HomeUi.Visible = false
	LDSHUB.AutoFarmUi.Visible = false
	LDSHUB.AutoEggUi.Visible = false
	LDSHUB.SeasonPassUi.Visible = false
	LDSHUB.AutoConvertUi.Visible = false
	LDSHUB.SettingsUI.Visible = false
	LDSHUB.UpdateNoteUi.Visible = false
	LDSHUB.RebirthsUi.Visible = false
	LDSHUB.AutoDeleteUi.Visible = false
	LDSHUB.TeleportsUi.Visible = false
	LDSHUB.AutoClaimUi.Visible = false
	LDSHUB.EventUi.Visible = false
	LDSHUB.AutoPotionsUi.Visible = false
	LDSHUB.AutoTrainUi.Visible = false
	LDSHUB.EventAtlantisUi.Visible = false
	LDSHUB.ArmsUi.Visible = false
end

local function resetButtons()
	LDSHUB.Home.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.AutoFarm.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.AutoEgg.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.SeasonPass.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Converts.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Settings.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.UpdateNotes.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Rebirths.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.AutoDelete.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Teleports.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.AutoClaim.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Event.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.AutoPotions.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.AutoTrain.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.EventAtlantis.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	LDSHUB.Arms.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
end

local function resetFrames(excludeFrame, excludeButton)
	hideFrames()
	resetButtons()

	if excludeFrame and excludeButton then
		excludeFrame.Visible = true
		excludeButton.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
	end
end

LDSHUB.Home.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.HomeUi, LDSHUB.Home)
	resetFrames(LDSHUB.HomeUi, LDSHUB.Home)
end)

LDSHUB.AutoFarm.MouseButton1Click:Connect(function()
	toggleFrame(LDSHUB.AutoFarmUi, LDSHUB.AutoFarm)
	resetFrames(LDSHUB.AutoFarmUi, LDSHUB.AutoFarm)
end)

